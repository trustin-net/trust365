{
	"name": "Trust365",
	"forwardPorts": [
		4321,
		35729
	],
	"portsAttributes": {
		"4321": {
			"label": "Server",
			"protocol": "https"
		},
		"35729": {
			"label": "LiveReload",
			"protocol": "https"
		}
	},
	"containerUser": "node",
	"image": "mcr.microsoft.com/devcontainers/typescript-node:1-16-bookworm",
	"postCreateCommand": "bash ./.devcontainer/post-create-command.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"bradlc.vscode-tailwindcss",
				"esbenp.prettier-vscode",
				"ms-vscode.azure-account"
			]
		}
	}
}